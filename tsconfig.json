{ "compilerOptions":
  { "module": "commonjs"
  , "outDir": "dist"
  , "sourceMap": true
  , "strict": true
  , "alwaysStrict": true
  , "noUnusedLocals": true
  , "noUnusedParameters": true
  , "noImplicitReturns": true
  , "noFallthroughCasesInSwitch": true
  , "baseUrl": "./"
  , "paths": 
    { "*": [ "node_modules/*" ]
    }
  }
, "include":
  [ "src/ts/**/*" ]
}
